{
  "name": "avg-maker",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "dist/electron/main.js",
  "scripts": {
    "predev": "tsc -p tsconfig.node.json && node build-preload-fix.cjs",
    "dev": "npm run predev && concurrently -k \"vite\" \"npm:electron:dev\"",
    "electron:dev": "wait-on http://localhost:5173 && electron-forge start",
    "build:web": "vite build && cp -R public dist",
    "make": "electron-forge make",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "d3-force": "^3.0.0",
    "dompurify": "^3.2.6",
    "file-saver": "^2.0.5",
    "inkjs": "^2.3.2",
    "jszip": "^3.10.1",
    "monaco-editor": "^0.52.2",
    "react": "^19.1.0",
    "react-d3-tree": "^3.6.6",
    "react-dom": "^19.1.0",
    "react-force-graph-2d": "^1.28.0",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.14",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.0",
    "electron": "^37.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "wait-on": "^8.0.2"
  }
}
